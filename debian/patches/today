+toxav_basic_test.log: toxav_basic_test$(EXEEXT)
+	@p='toxav_basic_test$(EXEEXT)'; \
+	b='toxav_basic_test'; \
+	$(am__check_pre) $(LOG_DRIVER) --test-name "$$f" \
