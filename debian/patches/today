+	done | $(am__base_list) | \
+	while read files; do \
+	  echo " $(INSTALL_HEADER) $$files '$(DESTDIR)$(libtoxcore_la_includedir)'"; \
+	  $(INSTALL_HEADER) $$files "$(DESTDIR)$(libtoxcore_la_includedir)" || exit $$?; \
+	done
