+mostlyclean-libtool:
+	-rm -f *.lo
+
+clean-libtool:
+	-rm -rf .libs _libs
+	-rm -rf ../toxav/.libs ../toxav/_libs
+	-rm -rf ../toxcore/.libs ../toxcore/_libs
+	-rm -rf ../toxdns/.libs ../toxdns/_libs
+	-rm -rf ../toxencryptsave/.libs ../toxencryptsave/_libs
+	-rm -rf ../toxencryptsave/crypto_pwhash_scryptsalsa208sha256/.libs ../toxencryptsave/crypto_pwhash_scryptsalsa208sha256/_libs
+	-rm -rf ../toxencryptsave/crypto_pwhash_scryptsalsa208sha256/nosse/.libs ../toxencryptsave/crypto_pwhash_scryptsalsa208sha256/nosse/_libs
+	-rm -rf ../toxencryptsave/crypto_pwhash_scryptsalsa208sha256/sse/.libs ../toxencryptsave/crypto_pwhash_scryptsalsa208sha256/sse/_libs
+install-libtoxav_la_includeHEADERS: $(libtoxav_la_include_HEADERS)
+	@$(NORMAL_INSTALL)
+	@list='$(libtoxav_la_include_HEADERS)'; test -n "$(libtoxav_la_includedir)" || list=; \
+	if test -n "$$list"; then \
+	  echo " $(MKDIR_P) '$(DESTDIR)$(libtoxav_la_includedir)'"; \
+	  $(MKDIR_P) "$(DESTDIR)$(libtoxav_la_includedir)" || exit 1; \
+	fi; \
+	for p in $$list; do \
+	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
+	  echo "$$d$$p"; \
+	done | $(am__base_list) | \
+	while read files; do \
+	  echo " $(INSTALL_HEADER) $$files '$(DESTDIR)$(libtoxav_la_includedir)'"; \
+	  $(INSTALL_HEADER) $$files "$(DESTDIR)$(libtoxav_la_includedir)" || exit $$?; \
+	done
+
+uninstall-libtoxav_la_includeHEADERS:
+	@$(NORMAL_UNINSTALL)
+	@list='$(libtoxav_la_include_HEADERS)'; test -n "$(libtoxav_la_includedir)" || list=; \
+	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
+	dir='$(DESTDIR)$(libtoxav_la_includedir)'; $(am__uninstall_files_from_dir)
+install-libtoxcore_la_includeHEADERS: $(libtoxcore_la_include_HEADERS)
