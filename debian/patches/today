+#	$(AM_V_CC)source='../toxav/group.c' object='../toxav/libtoxav_la-group.lo' libtool=yes \
+#	DEPDIR=$(DEPDIR) $(CCDEPMODE) $(depcomp) \
+#	$(AM_V_CC_no)$(LIBTOOL) $(AM_V_lt) --tag=CC $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=compile $(CC) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) $(AM_CPPFLAGS) $(CPPFLAGS) $(libtoxav_la_CFLAGS) $(CFLAGS) -c -o ../toxav/libtoxav_la-group.lo `test -f '../toxav/group.c' || echo '$(srcdir)/'`../toxav/group.c
